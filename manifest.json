{
  "manifest_version": 3,
  "name": "Grammar Corrector",
  "version": "1.0",
  "description": "A grammar correction tool.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
      
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}


